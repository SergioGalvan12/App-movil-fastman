{
  "expo": {
    "name": "fastman-app",
    "slug": "fastman-app",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/adaptive-icon.png",
    "userInterfaceStyle": "light",
    "plugins": [
      "expo-system-ui",
      "./withDisableForcedDarkModeAndroid"
    ],
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/fastman.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "userInterfaceStyle": "light",
      "infoPlist": {
        "NSCameraUsageDescription": "Necesitamos acceder a tu cámara para tomar fotos de la avería.",
        "NSPhotoLibraryUsageDescription": "Necesitamos acceder a tu galería para seleccionar fotos existentes.",
        "NSLocationWhenInUseUsageDescription": "Necesitamos tu ubicación para registrar el lugar del reporte.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Necesitamos tu ubicación para registrar el lugar del reporte."
      },
      "supportsTablet": true
    },
    "android": {
      "userInterfaceStyle": "light",
      "package": "com.mauricioasg2002.fastmanapp",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ],
      "softwareKeyboardLayoutMode": "resize",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/fastman.png"
    },
    "extra": {
      "eas": {
        "projectId": "cf3ecac9-667f-4a35-8b70-38b306da12d8"
      }
    },
    "owner": "mauricioasg2002"
  }
}